    {
        "name": "Phân số",
        "formula": "\\frac{a}{b}",
        "note": [
            "a:\\,Tử\\,số",
            "b:\\,Mẫu\\,số"
        ],
        "unit": "Bài 8: Số học"
    },
    {
        "name": "Cộng phân số",
        "formula": "\\frac{a}{b} + \\frac{c}{d} = \\frac{ad + bc}{bd}",
        "note": [
            "a, b, c, d:\\,Các\\,số\\,tự\\,nhiên\\,với\\,b \\neq 0\\,và\\,d \\neq 0"
        ],
        "unit": "Bài 9: Số học"
    },
    {
        "name": "Trừ phân số",
        "formula": "\\frac{a}{b} - \\frac{c}{d} = \\frac{ad - bc}{bd}",
        "note": [
            "a, b, c, d:\\,Các\\,số\\,tự\\,nhiên\\,với\\,b \\neq 0\\,và\\,d \\neq 0"
        ],
        "unit": "Bài 10: Số học"
    },
    {
        "name": "Nhân phân số",
        "formula": "\\frac{a}{b} \\cdot \\frac{c}{d} = \\frac{ac}{bd}",
        "note": [
            "a, b, c, d:\\,Các\\,số\\,tự\\,nhiên\\,với\\,b \\neq 0\\,và\\,d \\neq 0"
        ],
        "unit": "Bài 11: Số học"
    },
    {
        "name": "Chia phân số",
        "formula": "\\frac{a}{b} \\div \\frac{c}{d} = \\frac{a}{b} \\cdot \\frac{d}{c} = \\frac{ad}{bc}",
        "note": [
            "a, b, c, d:\\,Các\\,số\\,tự\\,nhiên\\,với\\,b \\neq 0\\,và\\,d \\neq 0"
        ],
        "unit": "Bài 12: Số học"
    },
    {
        "name": "Tỉ lệ thức",
        "formula": "\\frac{a}{b} = \\frac{c}{d}",
        "note": [
            "a, b, c, d:\\,Các\\,số\\,tự\\,nhiên\\,với\\,b \\neq 0\\,và\\,d \\neq 0"
        ],
        "unit": "Bài 13: Đại số"
    },
    {
        "name": "Tìm hai số khi biết tổng và tỉ số của chúng",
        "formula": "a = \\frac{T \\cdot t_1}{t_1 + t_2}, b = \\frac{T \\cdot t_2}{t_1 + t_2}",
        "note": [
            "a, b:\\,Hai\\,số\\,cần\\,tìm",
            "T:\\,Tổng\\,của\\,hai\\,số",
