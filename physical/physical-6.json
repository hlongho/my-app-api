[
    {
        "name": "Khối lượng",
        "formula": "m = \\rho \\cdot V",
        "note": [
            "m: Khối lượng (kg)",
            "\\rho: Khối lượng riêng (kg/m^3)",
            "V: Thể tích (m^3)"
        ],
        "unit": "Bài 1: Khối lượng"
    },
    {
        "name": "Thể tích hình hộp chữ nhật",
        "formula": "V = a \\cdot b \\cdot c",
        "note": [
            "V: Thể tích (m^3)",
            "a, b, c: Chiều dài, chiều rộng và chiều cao (m)"
        ],
        "unit": "Bài 2: Thể tích"
    },
    {
        "name": "Thể tích hình lập phương",
        "formula": "V = a^3",
        "note": [
            "V: Thể tích (m^3)",
            "a: Chiều dài cạnh (m)"
        ],
        "unit": "Bài 3: Thể tích"
    },
    {
        "name": "Thể tích hình cầu",
        "formula": "V = \\frac{4}{3} \\pi r^3",
        "note": [
            "V: Thể tích (m^3)",
            "r: Bán kính (m)"
        ],
        "unit": "Bài 4: Thể tích"
    },
    {
        "name": "Lực",
        "formula": "F = m \\cdot a",
        "note": [
            "F: Lực (N)",
            "m: Khối lượng (kg)",
            "a: Gia tốc (m/s^2)"
        ],
        "unit": "Bài 5: Lực"
    },
    {
        "name": "Trọng lượng",
        "formula": "P = m \\cdot g",
        "note": [
            "P: Trọng lượng (N)",
            "m: Khối lượng (kg)",
            "g: Gia tốc trọng trường (\\approx 9.8 m/s^2)"
        ],
        "unit": "Bài 6: Lực"
    },
    {
        "name": "Khối lượng riêng",
        "formula": "\\rho = \\frac{m}{V}",
        "note": [
            "\\rho: Khối lượng riêng (kg/m^3)",
            "m: Khối lượng (kg)",
            "V: Thể tích (m^3)"
        ],
        "unit": "Bài 7: Khối lượng riêng"
    },
    {
        "name": "Áp suất",
        "formula": "p = \\frac{F}{S}",
        "note": [
            "p: Áp suất (Pa)",
            "F: Lực tác dụng (N)",
            "S: Diện tích bề mặt (m^2)"
        ],
        "unit": "Bài 8: Áp suất"
    },
    {
        "name": "Công suất",
        "formula": "P = \\frac{W}{t}",
        "note": [
            "P: Công suất (W)",
            "W: Công (J)",
            "t: Thời gian (s)"
        ],
        "unit": "Bài 9: Công suất"
    },
    {
        "name": "Công cơ học",
        "formula": "W = F \\cdot s",
        "note": [
            "W: Công (J)",
            "F: Lực (N)",
            "s: Quãng đường (m)"
        ],
        "unit": "Bài 10: Công cơ học"
    },
    {
        "name": "Nhiệt năng",
        "formula": "Q = m \\cdot c \\cdot \\Delta t",
        "note": [
            "Q: Nhiệt năng (J)",
            "m: Khối lượng (kg)",
            "c: Nhiệt dung riêng (J/kg.°C)",
            "\\Delta t: Độ thay đổi nhiệt độ (°C)"
        ],
        "unit": "Bài 11: Nhiệt năng"
    },
    {
        "name": "Hiệu suất",
        "formula": "H = \\frac{A_{\\text{có ích}}}{A_{\\text{toàn phần}}} \\cdot 100\\%",
        "note": [
            "H: Hiệu suất (%)",
            "A_{\\text{có ích}}: Công có ích (J)",
            "A_{\\text{toàn phần}}: Công toàn phần (J)"
        ],
        "unit": "Bài 12: Hiệu suất"
    }
]
